{
  "name": "vite-env-only",
  "version": "0.0.0",
  "description": "Explicitly split up client and server code at the expression level",
  "keywords": [
    "vite-plugin",
    "env",
    "only",
    "client",
    "server",
    "macro"
  ],
  "repository": "pcattori/vite-env-only",
  "author": "pcattori",
  "license": "MIT",
