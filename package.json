    "build": "tsup src/index.ts --dts --format esm",
    "test": "vitest"
  },
  "packageManager": "pnpm@8.13.1",
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^20.10.6",
    "dedent": "^1.5.1",
    "recast": "^0.23.4",
    "semantic-release": "^22.0.12",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.2"
  },
  "dependencies": {
    "@babel/core": "^7.23.7",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.7",
    "@babel/types": "^7.23.6"
  },
  "prettier": {
    "semi": false
